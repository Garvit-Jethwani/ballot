build:
  type: thirdParty
  content: nrt1
deploy:
  type: text
  content: |-
    #/bin/sh

    HELM_OPTS="--set clusterIP=${ROOST_CLUSTER_IP},tag=${ROOST_NAMESPACE},namespace=${ROOST_NAMESPACE}"
    helm upgrade garvittest12-helm-vote helm-vote -n $ROOST_NAMESPACE --create-namespace ${HELM_OPTS}
test:
  - name: ballot
    configType: artillery
    configPath: ballot/test/test.yaml
uninstall:
  type: thirdParty
  content: '#!/bin/sh'
lastUpdatedBy: Garvit Jethwani
lastUpdatedOn: 2022-09-21T10:40:24.781Z
